#include <iostream>
using namespace std;

// Na podstawie algorytmu z pliku graficznego napisz funkcję,
// która przyjmuje liczbę całkowitą dodatnią, rozkłada ją na czynniki
// pierwsze. Funkcje nie zwraca wartości, lecz wyświetla czynniki.

void rozklad_na_czynniki_pierwsze(int N){
	int d=2;
	while(N>=0){
		if(N%d==0){
			cout << d << ", ";
			N=N/d; // dzielenie calkowite istnienie
		}else{
			d++;
		}
		
	}
	cout<<endl;
}
int main(){
rozklad_na_czynniki_pierwsze(10);
rozklad_na_czynniki_pierwsze(9);

	system("pause");
}



















































































































































































































































































































































































































































































































































































































































































































































































































































































































